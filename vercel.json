{
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "db.json"
                ]
            }
        }
    ],
    "rotas": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}